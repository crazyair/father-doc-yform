{
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crazyair/yforms.git"
  },
  "scripts": {
    "start": "dumi dev",
    "build": "father-build",
    "doc:build": "dumi build",
    "now-build": "yarn && yarn doc:build",
    "fa:build": "father build",
    "test": "umi-test",
    "lint": "eslint --ext .js,jsx,ts,tsx packages/**/src --fix",
    "lint:es": "eslint --ext .js,jsx,ts,tsx packages/**/src",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "changelog": "lerna-changelog",
    "pub": "npm run build && lerna publish"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    ".{less,css,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.1.1",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.0.3",
    "@types/warning": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@umijs/fabric": "^2.0.3",
    "babel-eslint": "^9.0.0",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^6.0.3",
    "dumi": "^1.0.25",
    "enzyme": "^3.11.0",
    "eslint": "^6.7.2",
    "eslint-config-umi": "^1.6.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "father-build": "^1.17.2",
    "husky": "^4.2.3",
    "lerna": "^3.20.2",
    "lerna-changelog": "^0.8.2",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "rc-util": "^4.19.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.7.0",
    "typescript": "^3.7.5",
    "umi": "^3.1.4",
    "umi-test": "^1.9.3"
  },
  "license": "MIT",
  "dependencies": {
    "antd": "^4.2.4",
    "dayjs": "^1.8.27",
    "rc-queue-anim": "^1.8.4"
  }
}
